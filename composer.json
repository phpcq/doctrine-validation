{
    "name": "phpcq/doctrine-validation",
    "description": "Validation tools for doctrine.",
    "license": "MIT",
    "authors": [
        {
            "name":"Tristan Lins",
            "email":"tristan@lins.io",
            "homepage":"https://tristan.lins.io",
            "role":"Developer"
        }
    ],
    "support":{
        "issues":"https://github.com/phpcq/doctrine-validation/issues",
        "source":"https://github.com/phpcq/doctrine-validation",
        "irc": "irc://irc.freenode.org/phpcq"
    },
    "autoload": {
        "psr-4": {
            "PhpCodeQuality\\DoctrineValidation\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpCodeQuality\\DoctrineValidation\\Test\\" : "tests"
        }
    },
    "require": {
        "php": ">=5.3.4",
        "symfony/console": "~2.3",
        "doctrine/annotations": "~1.0",
        "doctrine/orm": "~2.0"
    },
    "require-dev": {
        "phpcq/all-tasks": "~1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        }
    },
    "bin": ["bin/validate-doctrine-entity-column-names.php"],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
